@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix sdmkgc: <https://github.com/SDM-TIB/KGC/vocab/> .

<drug_TriplesMap1>
	rml:logicalSource [ rml:source "/app/data/drugs.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "https://github.com/SDM-TIB/KGC/entity/{DrugBankID}";
		rr:class sdmkgc:Drug
	];
    rr:predicateObjectMap [
        rr:predicate sdmkgc:drugLabel;
        rr:objectMap [
            rml:reference "DrugName";
        ]
    ];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
        rr:objectMap <FalconDBpedia-Function> ;
	];
    rr:predicateObjectMap [
        rr:predicate owl:sameAs;
        rr:objectMap <FalconWikipedia-Function> ;
    ];
    rr:predicateObjectMap [
        rr:predicate sio:SIO_000255;
        rr:objectMap <UMLSFunction> ;
    ].

##########################################################################################
######################################## Functions #######################################
##########################################################################################

<FalconDBpedia-Function>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/app/data/drugs.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant eablock:falcon_dbpedia_function 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate eablock:value;
            rr:objectMap [ 
                rml:reference "DrugName" 
            ] 
        ];    
    ].

#####

